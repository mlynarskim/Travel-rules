<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Manifest version -->
    <key>NSPrivacyManifestVersion</key>
    <string>1.0</string>

    <!-- Does the app use data for cross-app tracking as defined by ATT? Default: NO (non‑personalized ads). -->
    <key>NSPrivacyTracking</key>
    <false/>

    <!-- If you ever enable personalized ads or cross‑app attribution, add your tracking domains here and set NSPrivacyTracking to true. -->
    <!-- <key>NSPrivacyTrackingDomains</key>
    <array>
        <string>example-tracking-domain.your-partner.com</string>
    </array> -->

    <!-- Data your APP collects/uses (SDKs declare their own). Keep this minimal and accurate. -->
    <key>NSPrivacyCollectedDataTypes</key>
    <array>
        <!-- Product interaction: taps, screen views used to improve UX and basic analytics. -->
        <dict>
            <key>NSPrivacyCollectedDataType</key>
            <string>NSPrivacyCollectedDataTypeProductInteraction</string>
            <key>NSPrivacyCollectedDataTypeLinked</key>
            <false/>
            <key>NSPrivacyCollectedDataTypeTracking</key>
            <false/>
            <key>NSPrivacyCollectedDataTypePurposes</key>
            <array>
                <string>NSPrivacyCollectedDataTypePurposeAppFunctionality</string>
                <string>NSPrivacyCollectedDataTypePurposeAnalytics</string>
            </array>
        </dict>

        <!-- Diagnostics: crash logs, performance metrics generated by the app to fix bugs. -->
        <dict>
            <key>NSPrivacyCollectedDataType</key>
            <string>NSPrivacyCollectedDataTypeDiagnostics</string>
            <key>NSPrivacyCollectedDataTypeLinked</key>
            <false/>
            <key>NSPrivacyCollectedDataTypeTracking</key>
            <false/>
            <key>NSPrivacyCollectedDataTypePurposes</key>
            <array>
                <string>NSPrivacyCollectedDataTypePurposeAnalytics</string>
                <string>NSPrivacyCollectedDataTypePurposeAppFunctionality</string>
            </array>
        </dict>

        <!-- Advertising (non‑personalized): ad impressions/clicks for measurement only. -->
        <dict>
            <key>NSPrivacyCollectedDataType</key>
            <string>NSPrivacyCollectedDataTypeProductInteraction</string>
            <key>NSPrivacyCollectedDataTypeLinked</key>
            <false/>
            <key>NSPrivacyCollectedDataTypeTracking</key>
            <false/>
            <key>NSPrivacyCollectedDataTypePurposes</key>
            <array>
                <string>NSPrivacyCollectedDataTypePurposeAdvertising</string>
                <string>NSPrivacyCollectedDataTypePurposeAnalytics</string>
            </array>
        </dict>
    </array>

    <!-- Required Reason APIs: ONLY declare if your OWN APP code touches restricted APIs. 
         Leave empty if not applicable; third‑party SDKs ship their own manifests.  -->
    <!--
    <key>NSPrivacyAccessedAPITypes</key>
    <array>
        <dict>
            <key>NSPrivacyAccessedAPIType</key>
            <string>NSPrivacyAccessedAPICategoryDiskSpace</string>
            <key>NSPrivacyAccessedAPITypeReasons</key>
            <array>
                <string>E174.1</string>
            </array>
        </dict>
    </array>
    -->
</dict>
</plist>
